 RowBox[{
  RowBox[{"(*", 
   RowBox[{"distribution", " ", "functions"}], " ", "*)"}], 
  "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"uniformCDF", "[", "x_", "]"}], ":=", 
     RowBox[{"Piecewise", "[", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          FractionBox[
           RowBox[{"x", "-", "a"}], 
           RowBox[{"b", "-", "a"}]], ",", 
          RowBox[{"a", "<", "x", "<", "b"}]}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"1", ",", " ", 
          RowBox[{"x", ">=", "b"}]}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"0", ",", 
          RowBox[{"x", "<=", "a"}]}], "}"}]}], "}"}], "]"}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"uniformPDF", "[", "x_", "]"}], ":=", " ", 
     RowBox[{
      RowBox[{"uniformCDF", "'"}], "[", "x", "]"}]}], ";"}], 
   "\[IndentingNewLine]", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"increasingCDF", "[", "x_", "]"}], ":=", 
     RowBox[{"Piecewise", "[", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{
           RowBox[{"(", 
            FractionBox[
             RowBox[{"x", "-", "a"}], 
             RowBox[{"b", "-", "a"}]], ")"}], "^", "2"}], ",", 
          RowBox[{"a", "<", "x", "<", "b"}]}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"1", ",", " ", 
          RowBox[{"x", ">=", "b"}]}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"0", ",", 
          RowBox[{"x", "<=", "a"}]}], "}"}]}], "}"}], "]"}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"increasingPDF", "[", "x_", "]"}], ":=", 
     RowBox[{
      RowBox[{"increasingCDF", "'"}], "[", "x", "]"}]}], ";"}], 
   "\[IndentingNewLine]", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"decreasingCDF", "[", "x_", "]"}], ":=", 
     RowBox[{"Piecewise", "[", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          FractionBox[
           RowBox[{
            RowBox[{"(", 
             RowBox[{"a", "-", "x"}], ")"}], 
            RowBox[{"(", 
             RowBox[{"a", "+", "x", "-", 
              RowBox[{"2", "b"}]}], ")"}]}], 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"b", "-", "a"}], ")"}], "^", "2"}]], ",", 
          RowBox[{"a", "<", "x", "<", "b"}]}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"1", ",", " ", 
          RowBox[{"x", ">=", "b"}]}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"0", ",", 
          RowBox[{"x", "<=", "a"}]}], "}"}]}], "}"}], "]"}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"decreasingPDF", "[", "x_", "]"}], ":=", 
     RowBox[{
      RowBox[{"decreasingCDF", "'"}], "[", "x", "]"}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"decreasingPDF", "[", "x", "]"}], ",", 
      RowBox[{"increasingPDF", "[", "x", "]"}], ",", 
      RowBox[{"uniformPDF", "[", "x", "]"}]}], "}"}], "*)"}], 
   "\[IndentingNewLine]", 
